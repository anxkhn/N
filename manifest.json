{
  "manifest_version": 2,
  "name": "String Replace Extension",
  "version": "1.0",
  "description": "A Chrome extension to search and replace strings on webpages.",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/N_16.png",
      "48": "images/N_48.png",
      "128": "images/N_128.png"
    }
  },
  "icons": {
      "16": "images/N_16.png",
      "48": "images/N_48.png",
      "128": "images/N_128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
